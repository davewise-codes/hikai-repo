{
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact"
	],
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "never"
	},
	"eslint.workingDirectories": [{ "mode": "auto" }],
	"prettier.configPath": ".prettierrc",
	"antlr4.generation": {
		"mode": "none",
		"listeners": true,
		"visitors": false
	}
}
